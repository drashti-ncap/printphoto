<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.Usefull.Tshirt_category_activity">

    <Button
        android:id="@+id/btn_only_front"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_40sdp"
        android:text="Front_only" />

    <Button
        android:id="@+id/btn_only_back"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_40sdp"
        android:text="Back_only" />

    <Button
        android:id="@+id/btn_front_back"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_40sdp"
        android:text="Front_Back" />

    <Button
        android:id="@+id/btn_back_logo"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_40sdp"
        android:text="Back_Logo" />

    <Button
        android:id="@+id/btn_logo_only"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_40sdp"
        android:text="Logo_only" />

</LinearLayout>